    "type": "laravel-package",
    "license": "proprietary",
    "authors": [{
        "name": "Vizrex",
        "email": "laravel@vizrex.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "vizrex/laraviz": "~1",
        "benjamincrozat/laravel-dropbox-driver": "^2.0",
        "jackiedo/dotenv-editor": "1.*",
        "nao-pon/flysystem-google-drive": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Vizrex\\LaravelDbUtils\\": "src/"
        }
